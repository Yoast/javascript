{
  "name": "@yoast/components",
  "version": "0.13.1",
  "description": "Yoast Components",
  "main": "src/index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoast/javascript.git",
    "directory": "packages/components"
  },
  "author": "Yoast",
  "license": "GPL-3.0",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --max-warnings=96",
    "build": "webpack && rm dist/main.js",
    "prepublishOnly": "rm -rf dist && mkdir dist && find ./src/ -name \\*.js -exec rsync -R {} dist \\; && cp package.json dist/package.json && json -I -f dist/package.json -e \"this.main='src/index.js'\" && cp .babelrc dist/.babelrc && yarn build"
  },
  "jest": {
    "testRegex": ".*Test.js$",
    "testURL": "http://localhost",
    "setupTestFrameworkScriptFile": "<rootDir>/jest/setupTests.js"
  },
  "dependencies": {
    "@wordpress/a11y": "^1.1.3",
    "@wordpress/i18n": "^1.2.3",
    "@yoast/helpers": "^0.11.1",
    "@yoast/style-guide": "^0.11.1",
    "interpolate-components": "^1.1.1",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react-modal": "^3.8.1",
    "react-tabs": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@yoast/browserslist-config": "^1.2.1",
    "browserslist": "^4.7.3",
    "css-loader": "^3.4.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "glob": "^7.1.6",
    "grunt": "^1.0.3",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.8.4",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "16.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
